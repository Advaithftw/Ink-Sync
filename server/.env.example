# Example environment variables for Railway deployment
# Copy this to .env and fill in your actual values

# MongoDB connection string (use MongoDB Atlas for production)
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/inksync?retryWrites=true&w=majority

# JWT Secret key (generate a strong secret)
JWT_SECRET=your-super-secret-jwt-key-here

# Port (Railway will set this automatically, but good to have as fallback)
PORT=3001

# Node environment
NODE_ENV=production
